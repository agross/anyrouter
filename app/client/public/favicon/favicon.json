{
  "masterPicture": "public/favicon/black.svg",
  "iconsPath": "/",
  "design": {
    "ios": {
      "masterPicture": "public/favicon/white.svg",
      "pictureAspect": "backgroundAndMargin",
      "backgroundColor": "#008000",
      "margin": "14%",
      "assets": {
        "ios6AndPriorIcons": false,
        "ios7AndLaterIcons": false,
        "precomposedIcons": false,
        "declareOnlyDefaultIcon": true
      },
      "appName": "anyrouter"
    },
    "desktopBrowser": {},
    "windows": {
      "masterPicture": "public/favicon/white.svg",
      "pictureAspect": "noChange",
      "backgroundColor": "#00a300",
      "onConflict": "override",
      "assets": {
        "windows80Ie10Tile": false,
        "windows10Ie11EdgeTiles": {
          "small": false,
          "medium": true,
          "big": false,
          "rectangle": false
        }
      },
      "appName": "anyrouter"
    },
    "androidChrome": {
      "masterPicture": "public/favicon/white.svg",
      "pictureAspect": "backgroundAndMargin",
      "margin": "17%",
      "backgroundColor": "#008000",
      "themeColor": "#008000",
      "manifest": {
        "name": "anyrouter",
        "display": "browser",
        "orientation": "notSet",
        "onConflict": "override",
        "declared": true
      },
      "assets": {
        "legacyIcon": false,
        "lowResolutionIcons": false
      }
    },
    "safariPinnedTab": {
      "pictureAspect": "silhouette",
      "themeColor": "#008000"
    }
  },
  "settings": {
    "scalingAlgorithm": "Mitchell",
    "errorOnImageTooSmall": false,
    "readmeFile": false,
    "htmlCodeFile": false,
    "usePathAsIs": false
  }
}
